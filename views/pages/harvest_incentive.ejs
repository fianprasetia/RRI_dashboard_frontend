<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>

  <body>
    <div id="page-container" class="sidebar-o sidebar-dark enable-page-overlay side-scroll page-header-fixed page-footer-fixed sidebar-o side-trans-enabled">
      <nav id="sidebar" aria-label="Main Navigation"><%- include('../partials/navigasi'); %></nav>

      <!-- Header -->
      <header id="page-header">
        <!-- Header Content -->
        <%- include('../partials/header'); %>
      </header>
      <!-- END Header -->

      <!-- Main Container -->
      <main id="main-container">
        <!-- Page Content -->
        <div class="content">
          <h4 id="titlePage" class="content-heading text-uppercase"></h4>
          <div class="block block-rounded">
            <div class="block-header">
              <!-- <h3 class="block-title">add</h3> -->
              <div class="block-title">
                <button title="Add" type="submit" onclick="showModalInsertHarvestIncentive()" class="btn btn-primary">
                  <i class="fa-solid fa-plus"></i>
                </button>
              </div>
              <div class="block-options">
                <button type="button" class="btn-block-option" data-toggle="block-option" data-action="fullscreen_toggle"></button>
              </div>
            </div>
            <div class="block-content block-content-full">
              <div id="rowUsers" class="row table-responsive">
                <table class="table table-bordered table-striped table-vcenter js-dataTable-responsive">
                  <thead>
                    <tr>
                      <th class="text-center text-uppercase" rowspan="2">#</th>
                      <th id="companyThead" class="text-center text-uppercase" rowspan="2">Comapny</th>
                      <th id="harvestDayThead" class="text-center text-uppercase" rowspan="2">Harvest Day</th>
                      <th id="BJRThead" class="text-center text-uppercase" rowspan="2">BJR</th>
                      <th id="basisBonusThead" class="text-center text-uppercase" rowspan="2">Basis Bonus (Rp)</th>
                      <th id="harvestBasisThead" class="text-center text-uppercase" colspan="3">Harvest Basis (FFB)</th>
                      <!-- <th id="harvestBasisExtraThead" class="text-center text-uppercase"colspan="2">Harvest Basis Extra (FFB)</th> -->
                      <th id="extraBasisBonusThead" class="text-center text-uppercase" colspan="3">Extra Basis Bonus (Rp)</th>
                      <!-- <th id="extraBasisBonusExtraThead" class="text-center text-uppercase" colspan="2">Extra Basis Bonus Extra (Rp)</th> -->
                      <th id="looseFruitBonusThead" class="text-center text-uppercase" rowspan="2">Loose Fruit Bonus (Rp)</th>
                      <th id="createbyThead" class="text-center text-uppercase" rowspan="2">create By</th>
                      <th id="actionsThead" class="text-center text-uppercase" rowspan="2">Actions</th>
                    </tr>
                    <tr>
                      <td class="text-center text-uppercase">100%</td>
                      <td class="text-center text-uppercase">150%</td>
                      <td class="text-center text-uppercase">200%</td>
                      <td class="text-center text-uppercase">100%</td>
                      <td class="text-center text-uppercase">150%</td>
                      <td class="text-center text-uppercase">200%</td>
                    </tr>
                  </thead>
                  <tbody id="dataTable"></tbody>
                </table>
              </div>
            </div>
          </div>
          <!-- END Store Growth -->
        </div>
      </main>
      <footer id="page-footer" class="bg-body-light"><%- include('../partials/footer'); %></footer>
    </div>
    <div class="modal" id="modalHarvestIncentive" tabindex="-1" role="dialog" data-bs-backdrop="static" aria-labelledby="modalHarvestIncentive" aria-hidden="true">
      <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
          <div class="block block-rounded block-themed block-transparent mb-0">
            <div class="block-header bg-primary-dark">
              <h3 id="titleModal" class="block-title text-uppercase">Add Menu</h3>
              <div class="block-options">
                <button type="button" class="btn-block-option text-gray" data-toggle="block-option" data-action="fullscreen_toggle"></button>
              </div>
            </div>
            <form class="js-validation" id="form2">
              <div class="row block-content">
                <div class="col-sm-12 mb-2">
                  <label id="companyHeaderLabel" class="form-label text-uppercase" for="companyHeader">Company</label>
                  <select class="js-select2 form-select fw-light text-uppercase" name="companyHeader" id="companyHeader" style="width: 100%" data-container="#modalHarvestIncentive"></select>
                </div>
                <div class="col-sm-12 mb-2">
                  <label id="harvestDayHeaderLabel" class="form-label text-uppercase" for="harvestDayHeader">Harvest Day</label>
                  <select class="js-select2 form-select fw-light text-uppercase" name="harvestDayHeader" id="harvestDayHeader" style="width: 100%" data-container="#modalHarvestIncentive"></select>
                </div>
                <div class="col-sm-12 mb-2">
                  <label id="BJRHeaderLabel" class="form-label text-uppercase">BJR</label>
                  <div class="mb-2">
                    <div class="input-group">
                      <input type="text" class="form-control" onkeyup="validateDouble(this)" onfocus="removeZero(this)" value="0" id="startBJR" name="startBJR" />
                      <span class="input-group-text fw-semibold">
                        <i class="fa fa-fw fa-arrow-right"></i>
                      </span>
                      <input type="text" class="form-control" onkeyup="validateDouble(this)" onfocus="removeZero(this)" value="0" id="endBJR" name="endBJR" />
                    </div>
                  </div>
                </div>
                <div class="col-sm-12 mb-2">
                  <label id="basisBonusLabel" class="form-label text-uppercase" for="basisBonus">Basis Bonus (Rp)</label>
                  <input type="text" class="form-control fw-light text-uppercase" onkeyup="typeformatRupiah(this)" onfocus="removeZero(this)" value="0" id="basisBonus" name="basisBonus" />
                </div>
                <div class="col-sm-4 mb-2">
                  <label id="harvestBasisLabel" class="form-label text-uppercase" for="harvestBasis">Harvest Basis (FFB)</label>
                  <input type="text" class="form-control fw-light text-uppercase" onkeyup="validateDouble(this); harvestBasisIncentive()" onfocus="removeZero(this)" value="0" id="harvestBasis" name="harvestBasis" />
                </div>
                <div class="col-sm-4 mb-2">
                  <label id="harvestBasisILabel" class="form-label text-uppercase" for="harvestBasis">Harvest Basis I (FFB)</label>
                  <input type="text" class="form-control fw-light text-uppercase" onkeyup="validateDouble(this)" onfocus="removeZero(this)" value="0" id="harvestBasisI" name="harvestBasisI" />
                </div>
                <div class="col-sm-4 mb-2">
                  <label id="harvestBasisIILabel" class="form-label text-uppercase" for="harvestBasis">Harvest Basis II (FFB)</label>
                  <input type="text" class="form-control fw-light text-uppercase" onkeyup="validateDouble(this)" onfocus="removeZero(this)" value="0" id="harvestBasisII" name="harvestBasisII" />
                </div>
                <div class="col-sm-4 mb-2">
                  <label id="extraBasisBonusLabel" class="form-label text-uppercase" for="extraBasisBonus">Extra Basis Bonus (Rp)</label>
                  <input type="text" class="form-control fw-light text-uppercase" onkeyup="typeformatRupiah(this); harvestBasisIncentiveRp()" onfocus="removeZero(this)" value="0" id="extraBasisBonus" name="extraBasisBonus" />
                </div>

                <div class="col-sm-4 mb-2">
                  <label id="extraBasisBonusILabel" class="form-label text-uppercase" for="extraBasisBonus2">Extra Basis Bonus I (Rp)</label>
                  <input type="text" class="form-control fw-light text-uppercase" onkeyup="typeformatRupiah(this)" onfocus="removeZero(this)" value="0" id="extraBasisBonusI" name="extraBasisBonusI" />
                </div>

                <div class="col-sm-4 mb-2">
                  <label id="extraBasisBonusIILabel" class="form-label text-uppercase" for="extraBasisBonus2">Extra Basis Bonus II (Rp)</label>
                  <input type="text" class="form-control fw-light text-uppercase" onkeyup="typeformatRupiah(this)" onfocus="removeZero(this)" value="0" id="extraBasisBonusII" name="extraBasisBonusII" />
                </div>

                <div class="col-sm-12 mb-2">
                  <label id="looseFruitBonusLabel" class="form-label text-uppercase" for="looseFruitBonus">Loose Fruit Bonus (Rp)</label>
                  <input type="text" class="form-control fw-light text-uppercase" onkeyup="typeformatRupiah(this)" onfocus="removeZero(this)" value="0" id="looseFruitBonus" name="looseFruitBonus" />
                </div>
              </div>
              <div id="load" class="block-content block-content-full text-end bg-body"></div>
            </form>
          </div>
        </div>
      </div>
    </div>
    <!-- 1. CORE LIBRARIES (jQuery MUST be first) -->
    <script src="assets/js/lib/jquery.min.js"></script>
    <script src="assets/js/dashmix.app.min.js"></script>

    <!-- 2. UTILITY/HELPER SCRIPTS -->
    <script src="js/mainUrl.js"></script>
    <script src="lib/loadingPage.js"></script>
    <script src="lib/epps.js"></script>

    <!-- 3. FORM VALIDATION & INPUT HANDLING -->
    <script src="assets/js/plugins/jquery-validation/jquery.validate.min.js"></script>
    <script src="assets/js/plugins/jquery-validation/additional-methods.js"></script>
    <script src="assets/js/plugins/jquery.maskedinput/jquery.maskedinput.min.js"></script>
    <script src="assets/js/plugins/bootstrap-maxlength/bootstrap-maxlength.min.js"></script>

    <!-- 4. UI COMPONENTS -->
    <script src="assets/js/plugins/select2/js/select2.full.min.js"></script>
    <script src="assets/js/plugins/flatpickr/flatpickr.min.js"></script>
    <script src="assets/js/plugins/bootstrap-notify/bootstrap-notify.min.js"></script>

    <!-- 5. DATATABLES SUITE -->
    <script src="assets/js/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="assets/js/plugins/datatables-bs5/js/dataTables.bootstrap5.min.js"></script>
    <script src="assets/js/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="assets/js/plugins/datatables-responsive-bs5/js/responsive.bootstrap5.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons/dataTables.buttons.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-bs5/js/buttons.bootstrap5.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-jszip/jszip.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-pdfmake/pdfmake.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-pdfmake/vfs_fonts.js"></script>
    <script src="assets/js/plugins/datatables-buttons/buttons.print.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons/buttons.html5.min.js"></script>

    <!-- 6. DATA VISUALIZATION -->
    <script src="assets/js/plugins/chart.js/chart.umd.js"></script>

    <!-- 7. PAGE SPECIFIC SCRIPTS -->
    <script src="assets/js/pages/be_pages_dashboard.min.js"></script>

    <!-- 8. ASSET TYPE MODULE -->
    <script src="js/harvest_incentive/selectHarvestIncentive.js"></script>
    <script src="js/harvest_incentive/insertHarvestIncentive.js"></script>
    <script src="js/harvest_incentive/updateHarvestIncentive.js"></script>
    <script src="js/harvest_incentive/deleteHarvestIncentive.js"></script>

    <!-- 9. INITIALIZATION (Combined into single call) -->
    <script>
      Dashmix.helpersOnLoad(["js-flatpickr", "jq-datepicker", "jq-maxlength", "jq-select2", "jq-rangeslider", "jq-masked-inputs", "jq-pw-strength", "jq-notify"]);
    </script>
  </body>
</html>
