    <!-- 7. PAGE SPECIFIC SCRIPTS -->
    <script src="assets/js/pages/be_pages_dashboard.min.js"></script>

    <!-- 8. PAYMENT VOUCHER MODULE -->
    <script src="js/payment_voucher/selectPaymentVoucher.js"></script>
    <script src="js/payment_voucher/insertPaymentVoucher.js"></script>
    <script src="js/payment_voucher/updatePaymentVoucher.js"></script>
    <script src="js/payment_voucher/deletePaymentVoucher.js"></script>

