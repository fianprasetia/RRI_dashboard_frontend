<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>

  <body>
    <div id="page-container" class="sidebar-o sidebar-dark enable-page-overlay side-scroll page-header-fixed page-footer-fixed sidebar-o side-trans-enabled">
      <nav id="sidebar" aria-label="Main Navigation"><%- include('../partials/navigasi'); %></nav>
      <!-- END Sidebar -->

      <!-- Header -->
      <header id="page-header">
        <!-- Header Content -->
        <%- include('../partials/header'); %>
      </header>
      <!-- END Header -->

      <!-- Main Container -->
      <main id="main-container">
        <!-- Hero -->

        <!-- <div class="content">
          <div class="d-md-flex justify-content-md-between align-items-md-center pt-md-3 pb-md-0 text-center text-md-start content-heading">
            <div>
              <h4 id="titlePage" class="text-uppercase"></h4>
            </div>
          </div>
        </div> -->
        <!-- END Hero -->

        <!-- Page Content -->
        <div class="content">
          <h4 id="titlePage" class="content-heading text-uppercase"></h4>
          <div class="block block-rounded">
            <div class="block-header">
              <div id="mainNavigator" class="block-title">
                <button title="Add" type="submit" onclick="showLayoutCreateData()" class="btn btn-primary">
                  <i class="fa-solid fa-plus"></i>
                </button>
              </div>
              <div class="block-options">
                <button type="button" class="btn-block-option" data-toggle="block-option" data-action="fullscreen_toggle"></button>
              </div>
            </div>
            <div class="block-content block-content-full">
              <div id="createData" hidden>
                <div class="row mb-2 border border-1 rounded p-2">
                  <div class="col-sm-6">
                    <div class="row">
                      <label class="col-sm-4 col-form-label text-uppercase" id="estateHeaderLabel" for="estateHeader">Estate</label>
                      <div class="col-sm-8">
                        <select class="js-select2 form-select fw-light text-uppercase" name="estateHeader" onchange="selectDivision()" id="estateHeader" style="width: 100%" disabled></select>
                      </div>
                    </div>
                  </div>
                  <div class="col-sm-6">
                    <div class="row">
                      <label class="col-sm-4 col-form-label text-uppercase" id="divisiHeaderLabel" for="divisiHeader">Divisi</label>
                      <div class="col-sm-8">
                        <select class="js-select2 form-select fw-light text-uppercase" name="divisiHeader" onchange="selectBlock()" id="divisiHeader" style="width: 100%" disabled></select>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="row mb-2 border border-1 rounded p-2">
                  <form id="formHeaderBlockMaster">
                    <div class="row">
                      <div class="col-sm-6">
                        <div class="row mb-2">
                          <label class="col-sm-4 col-form-label text-uppercase" id="blockHeaderLabel" for="blockHeader">Block</label>
                          <div class="col-sm-8">
                            <select class="js-select2 form-select fw-light text-uppercase" name="blockHeader" id="blockHeader" style="width: 100%" disabled></select>
                          </div>
                        </div>
                        <div class="row mb-2">
                          <label class="col-sm-4 col-form-label text-uppercase" id="plantingYearHeaderLabel" for="plantingYearHeader">Tahun tanam</label>
                          <div class="col-sm-8">
                            <input type="text" class="form-control" id="plantingYearHeader" name="plantingYearHeader" data-autoclose="true" data-today-highlight="true" />
                          </div>
                        </div>
                        <div class="row mb-2">
                          <label class="col-sm-4 col-form-label text-uppercase" id="treeClassHeaderLabel" for="treeClassHeader">tree class</label>
                          <div class="col-sm-8">
                            <select class="js-select2 form-select fw-light text-uppercase" name="treeClassHeader" id="treeClassHeader" style="width: 100%"></select>
                          </div>
                        </div>
                        <div class="row mb-2">
                          <label class="col-sm-4 col-form-label text-uppercase" id="plantedAreaHeaderLabel" for="plantedAreaHeader">planted area</label>
                          <div class="col-sm-8">
                            <input type="text" class="form-control fw-light" id="plantedAreaHeader" name="plantedAreaHeader" value="0" onfocus="removeZero(this)" oninput="validateDouble(this)" />
                          </div>
                        </div>
                        <div class="row mb-2">
                          <label class="col-sm-4 col-form-label text-uppercase" id="unplantedAreaHeaderLabel" for="unplantedAreaHeader">unplanted area</label>
                          <div class="col-sm-8">
                            <input type="text" class="form-control fw-light" id="unplantedAreaHeader" name="unplantedAreaHeader" value="0" onfocus="removeZero(this)" oninput="validateDouble(this)" />
                          </div>
                        </div>
                        <div class="row mb-2">
                          <label class="col-sm-4 col-form-label text-uppercase" id="numberOfTreesHeaderLabel" for="numberOfTreesHeader">number of Trees</label>
                          <div class="col-sm-8">
                            <input type="text" class="form-control fw-light" id="numberOfTreesHeader" name="numberOfTreesHeader" value="0" onfocus="removeZero(this)" oninput="validateNumber(this)" />
                          </div>
                        </div>
                        <div class="row mb-2">
                          <label class="col-sm-4 col-form-label text-uppercase" id="blockStatusHeaderLabel" for="blockStatusHeader">block status</label>
                          <div class="col-sm-8">
                            <select class="js-select2 form-select fw-light text-uppercase" name="blockStatusHeader" id="blockStatusHeader" style="width: 100%"></select>
                          </div>
                        </div>
                      </div>
                      <div class="col-sm-6">
                        <div class="row mb-2">
                          <label class="col-sm-4 col-form-label text-uppercase" id="startHarvestHeaderLabel" for="startHarvestHeader">start harvest</label>
                          <div class="col-sm-8">
                            <input type="text" class="form-control text-uppercase" id="startHarvestHeader" name="startHarvestHeader" data-autoclose="true" data-today-highlight="true" />
                          </div>
                        </div>
                        <div class="row mb-2">
                          <label class="col-sm-4 col-form-label text-uppercase" id="soilCodeHeaderLabel" for="soilCodeHeader">soil code</label>
                          <div class="col-sm-8">
                            <select class="js-select2 form-select fw-light text-uppercase" name="soilCodeHeader" id="soilCodeHeader" style="width: 100%"></select>
                          </div>
                        </div>
                        <div class="row mb-2">
                          <label class="col-sm-4 col-form-label text-uppercase" id="soilClassificationHeaderLabel" for="soilClassificationHeader">soil Classification</label>
                          <div class="col-sm-8">
                            <select class="js-select2 form-select fw-light text-uppercase" name="soilClassificationHeader" id="soilClassificationHeader" style="width: 100%"></select>
                          </div>
                        </div>
                        <div class="row mb-2">
                          <label class="col-sm-4 col-form-label text-uppercase" id="topographyHeaderLabel" for="topographyHeader">topography</label>
                          <div class="col-sm-8">
                            <select class="js-select2 form-select fw-light text-uppercase" name="topographyHeader" id="topographyHeader" style="width: 100%"></select>
                          </div>
                        </div>
                        <div class="row mb-2">
                          <label class="col-sm-4 col-form-label text-uppercase" id="nucleusPlasmaHeaderLabel" for="nucleusPlasmaHeader">nucleus plasma</label>
                          <div class="col-sm-8">
                            <select class="js-select2 form-select fw-light text-uppercase" name="nucleusPlasmaHeader" id="nucleusPlasmaHeader" style="width: 100%"></select>
                          </div>
                        </div>
                        <div class="row mb-2">
                          <label class="col-sm-4 col-form-label text-uppercase" id="seedTypeHeaderLabel" for="seedTypeHeader">seed type</label>
                          <div class="col-sm-8">
                            <select class="js-select2 form-select fw-light text-uppercase" name="seedTypeHeader" id="seedTypeHeader" style="width: 100%"></select>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div id="load" class="col-sm-2 mb-2"></div>
                  </form>
                </div>
              </div>
              <div id="listData" hidden>
                <div id="caritanggal" class="row">
                  <!-- <div class="col-sm-6">
                    <div class="mb-2">
                      <div class="input-daterange input-group" data-date-format="dd-mm-yyyy" data-week-start="1" data-autoclose="true" data-today-highlight="true">
                        <input type="text" class="form-control" id="startDate" name="startDate" placeholder="From" data-week-start="1" data-autoclose="true" data-today-highlight="true" />
                        <span class="input-group-text fw-semibold"> <i class="fa fa-fw fa-arrow-right"></i> </span>
                        <input type="text" class="form-control" id="endDate" name="endDate" placeholder="To" data-week-start="1" data-autoclose="true" data-today-highlight="true" />
                      </div>
                    </div>
                  </div> -->
                  <!-- <div class="col-sm-3 mb-2">
                    <div id="loadsearch">
                      <span onclick="selectPaymentVoucherByDate()" class="input-group-text fw-semibold btn btn-primary"><i class="fa-solid fa-magnifying-glass"></i> </span>
                    </div>
                  </div> -->
                </div>
                <div class="row table-responsive">
                  <table id="dataTableGRHead" class="table table-bordered table-striped table-vcenter js-dataTable-responsive">
                    <thead>
                      <tr>
                        <th id="blockData" class="text-center text-uppercase">Block</th>
                        <th id="plantingYearData" class="text-center text-uppercase">Planting Year</th>
                        <th id="plantedAreaData" class="text-center text-uppercase">planted Area</th>
                        <th id="unplantedAreaData" class="text-center text-uppercase">unplanted area</th>
                        <th id="numberOfTreesData" class="text-center text-uppercase">number of trees</th>
                        <th id="blockStatusData" class="text-center text-uppercase">block status</th>
                        <th id="nucleusPlasmaData" class="text-center text-uppercase">nucleus plasma</th>
                        <th id="createByData" class="text-center text-uppercase">Create By</th>
                        <th id="actionsListData" class="text-center text-uppercase">Actions</th>
                      </tr>
                    </thead>
                    <tbody id="dataTable"></tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
          <!-- END Store Growth -->
        </div>
      </main>
      <footer id="page-footer" class="bg-body-light"><%- include('../partials/footer'); %></footer>
    </div>
    <!-- CORE LIBRARIES (jQuery MUST be first) -->
    <script src="assets/js/lib/jquery.min.js"></script>

    <!-- FRAMEWORK CORE (load early for plugin compatibility) -->
    <script src="assets/js/dashmix.app.min.js"></script>

    <!-- CUSTOM APPLICATION SCRIPTS -->
    <script src="js/mainUrl.js"></script>
    <script src="lib/loadingPage.js"></script>
    <script src="lib/epps.js"></script>

    <!-- FORM VALIDATION & INPUT ENHANCEMENTS -->
    <script src="assets/js/plugins/jquery-validation/jquery.validate.min.js"></script>
    <script src="assets/js/plugins/jquery-validation/additional-methods.js"></script>
    <script src="assets/js/plugins/bootstrap-maxlength/bootstrap-maxlength.min.js"></script>
    <script src="assets/js/plugins/jquery.maskedinput/jquery.maskedinput.min.js"></script>
    <script src="assets/js/plugins/select2/js/select2.full.min.js"></script>

    <!-- DATE/TIME PICKERS -->
    <script src="assets/js/plugins/bootstrap-datepicker/js/bootstrap-datepicker.min.js"></script>
    <script src="assets/js/plugins/flatpickr/flatpickr.min.js"></script>

    <!-- NOTIFICATION SYSTEM -->
    <script src="assets/js/plugins/bootstrap-notify/bootstrap-notify.min.js"></script>

    <!-- DATATABLES SUITE (with all dependencies) -->
    <script src="assets/js/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="assets/js/plugins/datatables-bs5/js/dataTables.bootstrap5.min.js"></script>
    <script src="assets/js/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="assets/js/plugins/datatables-responsive-bs5/js/responsive.bootstrap5.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons/dataTables.buttons.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-bs5/js/buttons.bootstrap5.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-jszip/jszip.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-pdfmake/pdfmake.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-pdfmake/vfs_fonts.js"></script>
    <script src="assets/js/plugins/datatables-buttons/buttons.print.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons/buttons.html5.min.js"></script>

    <!-- DATA VISUALIZATION -->
    <script src="assets/js/plugins/chart.js/chart.umd.js"></script>

    <!-- PAGE-SPECIFIC SCRIPTS -->
    <script src="assets/js/pages/be_pages_dashboard.min.js"></script>

    <!-- CORE APPLICATION SCRIPTS -->
    <script src="js/mainUrl.js"></script>

    <!-- APPLICATION MODULES (CRUD operations) -->
    <script src="js/block_master/selectBlockMaster.js"></script>
    <script src="js/block_master/insertBlockMaster.js"></script>
    <script src="js/block_master/updateBlockMaster.js"></script>
    <script src="js/block_master/deleteBlockMaster.js"></script>

    <!-- INITIALIZATION (Single optimized call) -->
    <script>
      Dashmix.helpersOnLoad(["js-flatpickr", "jq-datepicker", "jq-maxlength", "jq-select2", "jq-rangeslider", "jq-masked-inputs", "jq-pw-strength", "jq-notify"]);
      $("#plantingYearHeader").datepicker({
        format: "yyyy",
        startView: "years",
        minViewMode: "years",
        autoclose: true
      });
      $("#startHarvestHeader").datepicker({
        format: "mm-yyyy",
        startView: "months",
        minViewMode: "months",
        autoclose: true
      });
    </script>
  </body>
</html>
