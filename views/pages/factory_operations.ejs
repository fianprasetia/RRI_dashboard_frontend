<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>

  <body>
    <div id="page-container" class="sidebar-o sidebar-dark enable-page-overlay side-scroll page-header-fixed page-footer-fixed sidebar-o side-trans-enabled">
      <nav id="sidebar" aria-label="Main Navigation"><%- include('../partials/navigasi'); %></nav>
      <!-- END Sidebar -->

      <!-- Header -->
      <header id="page-header">
        <!-- Header Content -->
        <%- include('../partials/header'); %>
      </header>
      <!-- END Header -->

      <!-- Main Container -->
      <main id="main-container">
        <!-- Page Content -->
        <div class="content">
          <h4 id="titlePage" class="content-heading text-uppercase"></h4>
          <div class="block block-rounded">
            <div class="block-header">
              <div id="mainNavigator" class="block-title">
                <button title="Add" type="submit" onclick="showLayoutCreateData()" class="btn btn-primary">
                  <i class="fa-solid fa-plus"></i>
                </button>
              </div>
              <div class="block-options">
                <button type="button" class="btn-block-option" data-toggle="block-option" data-action="fullscreen_toggle"></button>
              </div>
            </div>
            <div class="block-content block-content-full">
              <div id="createData" hidden>
                <form id="formHeader">
                  <div class="row border border-1 rounded p-2 mb-2">
                    <div class="col-sm-6">
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="millHeaderLabel" for="millHeader">Mill</label>
                        <div class="col-sm-8">
                          <select class="js-select2 form-select fw-light text-uppercase" onchange="selectStation()" name="millHeader" id="millHeader" style="width: 100%"></select>
                        </div>
                      </div>
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="dateHeaderLabel" for="dateHeader">Date</label>
                        <div class="col-sm-8">
                          <input type="text" class="js-flatpickr form-control fw-light" onchange="checkDate()" id="dateHeader" name="dateHeader" data-date-format="d-m-Y" />
                        </div>
                      </div>
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="shiftHeaderLabel" for="shiftHeader">Shift</label>
                        <div class="col-sm-8">
                          <select class="js-select2 form-select fw-light text-uppercase" name="shiftHeader" id="shiftHeader" style="width: 100%"></select>
                        </div>
                      </div>
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="timeHeaderLabel" for="timeHeader">Time</label>
                        <div class="col-sm-8">
                          <div class="input-group" data-autoclose="true" data-today-highlight="true">
                            <input type="text" class="js-flatpickr form-control" placeholder="From" id="startTime" name="startTime" data-enable-time="true" data-no-calendar="true" data-date-format="H:i" data-time_24hr="true" />
                            <span class="input-group-text fw-semibold"> <i class="fa fa-fw fa-arrow-right"></i> </span>
                            <input type="text" class="js-flatpickr form-control" placeholder="To" onchange="countHours()" id="endTime" name="endTime" data-enable-time="true" data-no-calendar="true" data-date-format="H:i" data-time_24hr="true" />
                          </div>
                        </div>
                      </div>
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="foremanHeaderLabel" for="foremanHeader">Foreman</label>
                        <div class="col-sm-8">
                          <select class="js-select2 form-select fw-light text-uppercase" name="foremanHeader" id="foremanHeader" style="width: 100%"></select>
                        </div>
                      </div>
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="assistantHeaderLabel" for="assistantHeader">assistant</label>
                        <div class="col-sm-8">
                          <select class="js-select2 form-select fw-light text-uppercase" name="assistantHeader" id="assistantHeader" style="width: 100%"></select>
                        </div>
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="timeProcessHeaderLabel" for="timeProcessHeader">Time process</label>
                        <div class="col-sm-8">
                          <input type="text" class="form-control text-uppercase" id="timeProcessHeader" name="timeProcessHeader" value="0" disabled />
                        </div>
                      </div>
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="breakdownHeaderLabel" for="breakdownHeader">Breakdown</label>
                        <div class="col-sm-8">
                          <input type="text" class="js-flatpickr form-control" onchange="countHours()" id="breakdownHeader" name="breakdownHeader" value="00:00" data-enable-time="true" data-no-calendar="true" data-date-format="H:i" data-time_24hr="true" />
                        </div>
                      </div>
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="NumberofCagesHeaderLabel" for="NumberofCagesHeader">Number of cages</label>
                        <div class="col-sm-8">
                          <input type="text" class="form-control text-uppercase" id="NumberofCagesHeader" value="0" onkeyup="validateNumber(this);" onfocus="removeZero(this)" name="NumberofCagesHeader" />
                        </div>
                      </div>
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="processedFFBHeaderLabel" for="processedFFBHeader">Processed FFB (KG)</label>
                        <div class="col-sm-8">
                          <input type="text" class="form-control text-uppercase" id="processedFFBHeader" value="0" onkeyup="validateNumber(this);" onfocus="removeZero(this)" name="processedFFBHeader" />
                        </div>
                      </div>
                    </div>
                  </div>
                </form>
                <form id="formDetail">
                  <div class="row border border-1 rounded p-2 mb-2">
                    <div class="col-sm-6">
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="stationDetailLabel" for="stationDetail">Station</label>
                        <div class="col-sm-8">
                          <select class="js-select2 form-select fw-light text-uppercase" onchange="selectEngine()" name="stationDetail" id="stationDetail" style="width: 100%"></select>
                        </div>
                      </div>
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="machineDetailLabel" for="machineDetail">machine</label>
                        <div class="col-sm-8">
                          <select class="js-select2 form-select fw-light text-uppercase" name="machineDetail" id="machineDetail" style="width: 100%"></select>
                        </div>
                      </div>
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="timeRepairDetailLabel" for="timeRepairDetail">Time repair</label>
                        <div class="col-sm-8">
                          <div class="input-group" data-autoclose="true" data-today-highlight="true">
                            <input type="text" class="js-flatpickr form-control" placeholder="From" id="startRepairTime" name="startRepairTime" data-enable-time="true" data-no-calendar="true" data-date-format="H:i" data-time_24hr="true" />
                            <span class="input-group-text fw-semibold"> <i class="fa fa-fw fa-arrow-right"></i> </span>
                            <input type="text" class="js-flatpickr form-control" onchange="countHoursRepair()" placeholder="To" id="endRepairTime" name="endRepairTime" data-enable-time="true" data-no-calendar="true" data-date-format="H:i" data-time_24hr="true" />
                          </div>
                        </div>
                      </div>
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="breakdownDetailLabel" for="breakdownDetail">breakdown</label>
                        <div class="col-sm-8">
                          <input type="text" class="form-control text-uppercase" id="breakdownDetail" name="breakdownDetail" disabled />
                        </div>
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="presureDetailLabel" for="presureDetail">presure</label>
                        <div class="col-sm-8">
                          <div class="input-group" data-autoclose="true" data-today-highlight="true">
                            <input type="text" placeholder="From" class="form-control" id="startpresure" name="startpresure" />
                            <span class="input-group-text fw-semibold"> <i class="fa fa-fw fa-arrow-right"></i> </span>
                            <input type="text" placeholder="To" class="form-control" id="endpresure" name="endpresure" data-enable-time="true" data-no-calendar="true" data-date-format="H:i" data-time_24hr="true" />
                          </div>
                        </div>
                      </div>
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="noteDetailLabel" for="noteDetail">note</label>
                        <div class="col-sm-8">
                          <input type="text" class="form-control text-uppercase" id="noteDetail" name="noteDetail" />
                        </div>
                      </div>
                      <div class="row mb-2">
                        <label class="col-sm-4 col-form-label text-uppercase" id="statusDetailLabel" for="statusDetail">status</label>
                        <div class="col-sm-8">
                          <select class="js-select2 form-select fw-light text-uppercase" name="statusDetail" id="statusDetail" style="width: 100%"></select>
                        </div>
                      </div>
                    </div>
                  </div>
                </form>
                <div class="row table-responsive mb-2">
                  <table id="dynamicTable" class="table table-bordered table-striped table-vcenter">
                    <thead>
                      <tr>
                        <th rowspan="2" id="actionsOrder" class="text-center text-uppercase" style="width: 50px">
                          <span onclick="selectItemRepair()" class="fw-semibold btn btn-primary"><i class="fa-solid fa-plus"></i></span>
                        </th>
                        <th rowspan="2" id="stationThead" class="text-center text-uppercase">Station</th>
                        <th rowspan="2" id="machineThead" class="text-center text-uppercase">machine</th>
                        <th colspan="2" id="timeRepairThead" class="text-center text-uppercase">timeRepai</th>
                        <th rowspan="2" id="downtimeThead" class="text-center text-uppercase">downtime</th>
                        <th colspan="2" id="presuretime" class="text-center text-uppercase">presure</th>
                        <th rowspan="2" id="noteThead" class="text-center text-uppercase">Note</th>
                        <th rowspan="2" id="statusThead" class="text-center text-uppercase">status</th>
                      </tr>
                      <tr>
                        <th class="text-center fw-light text-uppercase">From</th>
                        <th class="text-center fw-light text-uppercase">To</th>
                        <th class="text-center fw-light text-uppercase">From</th>
                        <th class="text-center fw-light text-uppercase">To</th>
                      </tr>
                    </thead>
                    <tbody id="dataTableItem"></tbody>
                  </table>
                </div>
                <div id="load" class="mb-2 space-x-1"></div>
              </div>
              <div id="listData" hidden>
                <div class="row">
                  <div class="col-sm-6">
                    <div class="mb-2">
                      <div class="input-daterange input-group" data-date-format="dd-mm-yyyy" data-week-start="1" data-autoclose="true" data-today-highlight="true">
                        <input type="text" class="form-control" id="startDate" name="startDate" placeholder="From" data-week-start="1" data-autoclose="true" data-today-highlight="true" />
                        <span class="input-group-text fw-semibold"> <i class="fa fa-fw fa-arrow-right"></i> </span>
                        <input type="text" class="form-control" id="endDate" name="endDate" placeholder="To" data-week-start="1" data-autoclose="true" data-today-highlight="true" />
                      </div>
                    </div>
                  </div>
                  <div class="col-sm-3 mb-2">
                    <div id="loadsearch">
                      <span onclick="selectCashBankByDate()" class="input-group-text fw-semibold btn btn-primary"><i class="fa-solid fa-magnifying-glass"></i> </span>
                    </div>
                  </div>
                </div>
                <div class="row table-responsive">
                  <table id="dataTableGRHead" class="table table-bordered table-striped table-vcenter js-dataTable-responsive">
                    <thead>
                      <tr>
                        <th rowspan="2" class="text-center text-uppercase">#</th>
                        <th rowspan="2" id="noTransactionTable" class="text-center text-uppercase">No Transaksi</th>
                        <th rowspan="2" id="locationTable" class="text-center text-uppercase">Location</th>
                        <th rowspan="2" id="dateTable" class="text-center text-uppercase">date</th>
                        <th colspan="2" id="workShiftTable" class="text-center text-uppercase">work_shift</th>
                        <th rowspan="2" id="downtimeTable" class="text-center text-uppercase">downtime</th>
                        <th rowspan="2" id="foremanTable" class="text-center text-uppercase">foreman</th>
                        <th rowspan="2" id="assistantTable" class="text-center text-uppercase">assistant</th>
                        <th rowspan="2" id="createByTable" class="text-center text-uppercase">Create By</th>
                        <th rowspan="2" id="updateByTable" class="text-center text-uppercase">update By</th>
                        <th rowspan="2" id="actionTable" class="text-center text-uppercase">Action</th>
                      </tr>
                      <tr>
                        <th class="text-center fw-light text-uppercase">From</th>
                        <th class="text-center fw-light text-uppercase">To</th>
                      </tr>
                    </thead>
                    <tbody id="dataTable"></tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
          <!-- END Store Growth -->
        </div>
      </main>
      <footer id="page-footer" class="bg-body-light"><%- include('../partials/footer'); %></footer>
    </div>
    <div class="modal" id="modalNotice" tabindex="-1" role="dialog" aria-labelledby="modalNotice" data-bs-backdrop="static" aria-hidden="true">
      <div class="modal-dialog modal-dialog-top" role="document">
        <div class="modal-content">
          <div class="block block-rounded block-themed block-transparent mb-0">
            <div class="block-header bg-primary-dark">
              <h3 id="titleContent" class="block-title text-uppercase">Modal Title</h3>
              <div class="block-options"></div>
            </div>
            <div id="contentNotice" class="block-content"></div>
            <div id="loadNotice" class="block-content block-content-full text-end bg-body"></div>
          </div>
        </div>
      </div>
    </div>

    <!-- CORE LIBRARIES (jQuery MUST be first) -->
    <script src="assets/js/lib/jquery.min.js"></script>
    <script src="assets/js/dashmix.app.min.js"></script>

    <!-- APPLICATION CORE -->
    <script src="js/mainUrl.js"></script>
    <script src="lib/loadingPage.js"></script>
    <script src="lib/epps.js"></script>

    <!-- FORM VALIDATION & INPUT HANDLING -->
    <script src="assets/js/plugins/jquery-validation/jquery.validate.min.js"></script>
    <script src="assets/js/plugins/jquery-validation/additional-methods.js"></script>
    <script src="assets/js/plugins/jquery.maskedinput/jquery.maskedinput.min.js"></script>
    <script src="assets/js/plugins/bootstrap-maxlength/bootstrap-maxlength.min.js"></script>

    <!-- UI COMPONENTS -->
    <script src="assets/js/plugins/select2/js/select2.full.min.js"></script>
    <script src="assets/js/plugins/flatpickr/flatpickr.min.js"></script>
    <script src="assets/js/plugins/bootstrap-datepicker/js/bootstrap-datepicker.min.js"></script>
    <script src="assets/js/plugins/bootstrap-notify/bootstrap-notify.min.js"></script>

    <!-- DATATABLES SUITE -->
    <script src="assets/js/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="assets/js/plugins/datatables-bs5/js/dataTables.bootstrap5.min.js"></script>
    <script src="assets/js/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="assets/js/plugins/datatables-responsive-bs5/js/responsive.bootstrap5.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons/dataTables.buttons.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-bs5/js/buttons.bootstrap5.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-jszip/jszip.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-pdfmake/pdfmake.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-pdfmake/vfs_fonts.js"></script>
    <script src="assets/js/plugins/datatables-buttons/buttons.print.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons/buttons.html5.min.js"></script>

    <!-- DATA VISUALIZATION -->
    <script src="assets/js/plugins/chart.js/chart.umd.js"></script>

    <!-- PAGE SPECIFIC SCRIPTS -->
    <script src="assets/js/pages/be_pages_dashboard.min.js"></script>

    <!-- CASH BANK MODULE -->
    <script src="js/factory_operations/selectFactoryOperations.js"></script>
    <script src="js/factory_operations/insertFactoryOperations.js"></script>
    <script src="js/factory_operations/updateFactoryOperations.js"></script>
    <script src="js/factory_operations/deleteFactoryOperations.js"></script>

    <!-- INITIALIZATION (Single combined call) -->
    <script>
      Dashmix.helpersOnLoad(["js-flatpickr", "jq-datepicker", "jq-maxlength", "jq-select2", "jq-rangeslider", "jq-masked-inputs", "jq-pw-strength", "jq-notify"]);
    </script>
  </body>
</html>
