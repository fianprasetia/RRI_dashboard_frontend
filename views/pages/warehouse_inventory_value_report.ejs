<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>

  <body>
    <div id="page-container" class="sidebar-o sidebar-dark enable-page-overlay side-scroll page-header-fixed page-footer-fixed sidebar-o side-trans-enabled">
      <nav id="sidebar" aria-label="Main Navigation"><%- include('../partials/navigasi'); %></nav>
      <!-- END Sidebar -->

      <!-- Header -->
      <header id="page-header">
        <!-- Header Content -->
        <%- include('../partials/header'); %>
      </header>
      <!-- END Header -->

      <!-- Main Container -->
      <main id="main-container">
        <!-- Page Content -->
        <div class="content">
          <h4 id="titlePage" class="content-heading text-uppercase"></h4>
          <div class="block block-rounded">
            <div class="block-header">
              <div class="block-title"></div>
              <div class="block-options">
                <button type="button" class="btn-block-option" data-toggle="block-option" data-action="fullscreen_toggle"></button>
              </div>
            </div>
            <div class="block-content block-content-full">
              <form class="js-validation" id="form2">
                <div class="row mb-4">
                  <div class="col-sm-2">
                    <input type="text" class="form-control" id="periodDateHeader" onchange="selectCompany()" name="periodDateHeader" placeholder="Period" />
                  </div>
                  <div class="col-sm-3">
                    <select class="js-select2 form-select fw-light text-uppercase" onchange="selectWorksite()" name="companyHeader" id="companyHeader" style="width: 100%"></select>
                  </div>
                  <div class="col-sm-3">
                    <select class="js-select2 form-select fw-light text-uppercase" onchange="selectItemMaster()" name="locationHeader" id="locationHeader" style="width: 100%"></select>
                  </div>
                  <div class="col-sm-3">
                    <select class="js-select2 form-select fw-light text-uppercase" name="itemMasterHeader" id="itemMasterHeader" style="width: 100%"></select>
                  </div>
                  <div class="col-sm-1">
                    <div id="loadsearch">
                      <span onclick="selectWarehouseItem()" class="input-group-text fw-semibold btn btn-primary"> <i class="fa-solid fa-magnifying-glass"></i> </span>
                    </div>
                  </div>
                </div>
              </form>
              <div hidden id="doc" class="mb-2">
                <button onclick="selectWarehouseInventoryValueExcel()" class="btn" href="#"><img class="" style="width: 35px" src="file/icon/xls.png" /></button>
              </div>
              <div class="row table-responsive">
                <table class="table table-bordered table-striped table-vcenter">
                  <thead>
                    <tr>
                      <th class="text-center text-uppercase" rowspan="2">#</th>
                      <th id="codeCOAHead" class="text-center text-uppercase" rowspan="2">kode COA</th>
                      <th id="codeItemHead" class="text-center text-uppercase" rowspan="2">kode barang</th>
                      <th id="descriptionsHead" class="text-center text-uppercase" rowspan="2">descriptions</th>
                      <th id="uomHead" class="text-center text-uppercase" rowspan="2">uom</th>
                      <th id="beginningHead" class="text-center text-uppercase" colspan="3">saldo awal</th>
                      <th id="inHead" class="text-center text-uppercase" colspan="3">IN</th>
                      <th id="outHead" class="text-center text-uppercase" colspan="3">OUT</th>
                      <th id="endingHead" class="text-center text-uppercase" colspan="3">saldo akhir</th>
                    </tr>
                    <tr>
                      <td id="qtyBeginning" class="text-center text-uppercase">qty</td>
                      <td id="unitPriceBeginning" class="text-center text-uppercase">unit price</td>
                      <td id="totalPriceBeginning" class="text-center text-uppercase">total Price</td>
                      <td id="qtyIncomming" class="text-center text-uppercase">qty</td>
                      <td id="unitPriceIncomming" class="text-center text-uppercase">unit price</td>
                      <td id="totalPriceIncomming" class="text-center text-uppercase">total Price</td>
                      <td id="qtyOutgoing" class="text-center text-uppercase">qty</td>
                      <td id="unitPriceOutgoing" class="text-center text-uppercase">unit price</td>
                      <td id="totalPriceOutgoing" class="text-center text-uppercase">total Price</td>
                      <td id="qtyEnding" class="text-center text-uppercase">qty</td>
                      <td id="unitPriceEnding" class="text-center text-uppercase">unit price</td>
                      <td id="totalPriceEnding" class="text-center text-uppercase">total Price</td>
                    </tr>
                  </thead>
                  <tbody id="dataTable"></tbody>
                  <tfoot id="dataTableFoot"></tfoot>
                </table>
              </div>
            </div>
          </div>
          <!-- END Store Growth -->
        </div>
      </main>
      <footer id="page-footer" class="bg-body-light"><%- include('../partials/footer'); %></footer>
    </div>
    <!-- Core Libraries (jQuery must load first) -->
    <script src="assets/js/lib/jquery.min.js"></script>
    <script src="assets/js/dashmix.app.min.js"></script>

    <!-- Application Core -->
    <script src="js/mainUrl.js"></script>
    <script src="lib/loadingPage.js"></script>
    <script src="lib/epps.js"></script>

    <!-- Form Controls & Validation -->
    <script src="assets/js/plugins/select2/js/select2.full.min.js"></script>
    <script src="assets/js/plugins/bootstrap-maxlength/bootstrap-maxlength.min.js"></script>
    <script src="assets/js/plugins/jquery-validation/jquery.validate.min.js"></script>
    <script src="assets/js/plugins/jquery-validation/additional-methods.js"></script>
    <script src="assets/js/plugins/jquery.maskedinput/jquery.maskedinput.min.js"></script>

    <!-- Date/Time Pickers -->
    <script src="assets/js/plugins/bootstrap-datepicker/js/bootstrap-datepicker.min.js"></script>
    <script src="assets/js/plugins/flatpickr/flatpickr.min.js"></script>

    <!-- Data Tables & Export -->
    <script src="assets/js/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="assets/js/plugins/datatables-bs5/js/dataTables.bootstrap5.min.js"></script>
    <script src="assets/js/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="assets/js/plugins/datatables-responsive-bs5/js/responsive.bootstrap5.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons/dataTables.buttons.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-bs5/js/buttons.bootstrap5.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-jszip/jszip.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-pdfmake/pdfmake.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons-pdfmake/vfs_fonts.js"></script>
    <script src="assets/js/plugins/datatables-buttons/buttons.print.min.js"></script>
    <script src="assets/js/plugins/datatables-buttons/buttons.html5.min.js"></script>

    <!-- Visualization & Notifications -->
    <script src="assets/js/plugins/chart.js/chart.umd.js"></script>
    <script src="assets/js/plugins/bootstrap-notify/bootstrap-notify.min.js"></script>

    <!-- Page Specific Scripts -->
    <script src="assets/js/pages/be_pages_dashboard.min.js"></script>

    <!-- Warehouse Inventory Value Report Modules -->
    <script src="js/warehouse_inventory_value_report/selectWarehouseInventoryValueReport.js"></script>
    <script src="js/warehouse_inventory_value_report/insertWarehouseInventoryValueReport.js"></script>
    <script src="js/warehouse_inventory_value_report/updateWarehouseInventoryValueReport.js"></script>
    <script src="js/warehouse_inventory_value_report/deleteWarehouseInventoryValueReport.js"></script>

    <!-- Initialization -->
    <script>
      // Initialize datepicker
      $("#periodDateHeader").datepicker({
        format: "mm-yyyy",
        startView: "months",
        minViewMode: "months",
        autoclose: true
      });

      // Initialize Dashmix helpers
      Dashmix.helpersOnLoad(["js-flatpickr", "jq-datepicker", "jq-maxlength", "jq-select2", "jq-rangeslider", "jq-masked-inputs", "jq-pw-strength", "jq-notify"]);
    </script>
  </body>
</html>
